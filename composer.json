{
    "name": "goodcat/laravel-querystring",
    "description": "A laravel package to filter Eloquent models using query string parameters",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Goodcat\\QueryString\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goodcat\\QueryString\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Matteo Palazzo",
            "email": "outlaw.plz@gmail.com"
        }
    ],
    "homepage": "https://github.com/goodcat-dev/laravel-querystring",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Goodcat\\QueryString\\QueryStringServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "query-string",
        "filters",
        "goodcat"
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "laravel/pint": "^1.22",
        "larastan/larastan": "^3.5",
        "pestphp/pest": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
