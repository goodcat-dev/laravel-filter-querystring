{
    "name": "goodcat/laravel-querystring",
    "description": "A Laravel package to map query string to filter function",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Goodcat\\QueryString\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goodcat\\QueryString\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Matteo Palazzo",
            "email": "outlaw.plz@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Goodcat\\QueryString\\QueryStringServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "query-string",
        "filters",
        "goodcat"
    ],
    "require-dev": {
        "orchestra/testbench": "^9.9"
    }
}
